{
  "presets": [
    ["env", {
      "targets": { "node": 7 },
      "useBuiltIns": true
    }],
    "stage-0",
    "react",
    "es2015"
  ],
  "plugins": ["add-module-exports", "dynamic-import-webpack"],
  "env": {
    "production": {
      "presets": ["react-optimize", "react-hmre"],
      "plugins": ["babel-plugin-dev-expression"]
    },
    "development": {
      "plugins": [
        "transform-class-properties",
        "transform-es2015-classes",
        ["flow-runtime", {
          "assert": true,
          "annotate": true
        }]
      ]
    },
    "test": {
      "plugins": [
        ["webpack-loaders", { "config": "webpack.config.node.js", "verbose": false }]
      ]
    }
  }
}




"presets": ["es2015", "stage-0", "react"],
"plugins": ["add-module-exports"],
"env": {
  "development": {
    "presets": ["react-hmre"]
  },
  "test": {
    "plugins": [
      ["webpack-loaders", { "config": "webpack.config.node.js", "verbose": false }]
    ]
  }
}
